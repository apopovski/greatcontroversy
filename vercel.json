{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/rs",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|Twitterbot|WhatsApp|Slackbot|Discordbot|LinkedInBot|TelegramBot|SkypeUriPreview|Googlebot).*"
        }
      ],
      "destination": "/og/sr.html"
    },
    {
      "source": "/rs/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|Twitterbot|WhatsApp|Slackbot|Discordbot|LinkedInBot|TelegramBot|SkypeUriPreview|Googlebot).*"
        }
      ],
      "destination": "/og/sr.html"
    },
    {
      "source": "/:lang(en|es|de|it|da|no|pt|sm|et|ro|hr|bg|sk|cs|uk|ru|pl|ar|am|zh|sr|fa|af|hi|bn|id)",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|Twitterbot|WhatsApp|Slackbot|Discordbot|LinkedInBot|TelegramBot|SkypeUriPreview|Googlebot).*"
        }
      ],
      "destination": "/og/:lang.html"
    },
    {
      "source": "/:lang(en|es|de|it|da|no|pt|sm|et|ro|hr|bg|sk|cs|uk|ru|pl|ar|am|zh|sr|fa|af|hi|bn|id)/",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": ".*(facebookexternalhit|Facebot|Twitterbot|WhatsApp|Slackbot|Discordbot|LinkedInBot|TelegramBot|SkypeUriPreview|Googlebot).*"
        }
      ],
      "destination": "/og/:lang.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
